<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

    <application>addama-systemsbiology-hrd</application>
    <version>11</version>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="org.systemsbiology.addama.appengine.servlet.externalcontent.base"
                  value="http://addama.googlecode.com/svn/tags/2.1/user-interfaces/src/main/html"/>
        <property name="org.systemsbiology.addama.appengine.servlet.externalcontent.js"
                  value="https://informatics-apps.systemsbiology.net"/>        
    </system-properties>
    <threadsafe>true</threadsafe>
    <precompilation-enabled>true</precompilation-enabled>

</appengine-web-app>
